{
  "name": "server",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "node dist/src/index.js",
    "dev": "nodemon -r dotenv/config --exec ts-node src/index.ts",
    "build": "tsc ",
    "test": "yarn prisma:test && dotenv -e .env.test -- jest --runInBand",
    "format": "prettier --write \"{prisma,src,test}/**/*.{ts,tsx,prisma}\"",
    "docker:up": "docker compose up --detach --no-recreate --wait",
    "docker:down": "docker compose down --timeout 5",
    "prisma:format": "prisma format",
    "prisma:generate": "prisma generate",
    "prisma:push": "prisma db push && yarn prisma:generate",
    "prisma:seed": "prisma db seed",
    "prisma:dev": "yarn docker:up && dotenv -e .env -- yarn prisma migrate dev",
    "prisma:dev:studio": "yarn docker:up && dotenv -e .env -- prisma studio",
    "prisma:dev:reset": "yarn docker:up && dotenv -e .env -- prisma migrate reset --force",
    "prisma:test": "yarn docker:up && dotenv -e .env.test -- prisma migrate reset --force --skip-seed",
    "prisma:test:studio": "yarn docker:up && dotenv -e .env.test -- prisma studio",
    "prisma:push:loss": "prisma db push --accept-data-loss && yarn prisma:generate"
  },
  "prisma": {
    "seed": "ts-node prisma/seed/index.ts"
  },
  "dependencies": {
    "@prisma/client": "4.8.1",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/morgan": "^1.9.4",
    "@types/multer": "^1.4.7",
    "@types/node": "^18.14.6",
    "aws-sdk": "^2.1361.0",
    "axios": "^1.3.4",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "express-oauth2-jwt-bearer": "^1.3.0",
    "helmet": "^6.0.1",
    "jsonwebtoken": "^9.0.0",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "nodemon": "^2.0.20",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5",
    "zod": "^3.20.6"
  },
  "devDependencies": {
    "@faker-js/faker": "^7.6.0",
    "@mermaid-js/mermaid-cli": "^9.3.0",
    "@trivago/prettier-plugin-sort-imports": "^4.0.0",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.4.0",
    "@types/jsonwebtoken": "^9.0.1",
    "@types/supertest": "^2.0.12",
    "dotenv-cli": "^7.0.0",
    "jest": "^29.4.2",
    "prettier": "^2.8.4",
    "prettier-plugin-prisma": "^4.10.0",
    "prisma": "4.8.1",
    "prisma-erd-generator": "^1.2.5",
    "supertest": "^6.3.3",
    "ts-jest": "^29.0.5",
    "typescript": "^4.9.5"
  }
}
